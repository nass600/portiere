{
    "name": "nass600/vhost-builder",
    "description": "Vhost builder command based on Console Component",
    "type": "library",
    "keywords": ["symfony", "command", "vhost", "nginx", "apache", "virtual host"],
    "homepage": "http://github.com/nass600/vhost-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignacio Velazquez",
            "email": "ivelazquez85@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "3.*",
        "symfony/templating": "3.*",
        "symfony/filesystem": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": { "VhostBuilder\\": "src/VhostBuilder" }
    },
    "autoload-dev": {
        "psr-4": {"VhostBuilder\\": "tests/VhostBuilder"}
    },
    "bin": [
        "vhost-create",
        "vhost-delete"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
