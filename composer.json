{
    "name": "nass600/vhost-builder",
    "description": "Vhost builder command based on Console Component",
    "type": "library",
    "keywords": ["symfony", "command", "vhost", "nginx", "apache"],
    "homepage": "http://github.com/nass600/vhost-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignacio Velazquez",
            "email": "ivelazquez85@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "~2.6",
        "symfony/templating": "~2.6",
        "symfony/filesystem": "~2.6"
    },
    "autoload": {
        "psr-4": {
            "Nass600\\": "src/Nass600"
        }
    },
    "bin": [
        "bin/vhost-builder"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
